emp) => {\r\n        if (tempFormat) {\r\n            let finalTemp = (temp - 273.15).toFixed();\r\n            return finalTemp !== \"-0\" ? finalTemp : finalTemp = 0;\r\n        }\r\n        else if (!tempFormat) {\r\n            let finalTemp = (((temp - 273.15) * 9) / 5 + 32).toFixed();\r\n            return finalTemp !== \"-0\" ? finalTemp : finalTemp = 0;\r\n        }\r\n    };\r\n\r\n    const rainChance = (value) => {\r\n        const min = value.temp.min - 273.15;\r\n        const dew = value.dew_point - 273.15;\r\n        let chance = ((dew/min) * 100).toFixed();\r\n        if (chance > 100) {\r\n            chance = 100;\r\n        }\r\n        return chance >= 50 ? chance+\"%\" : null\r\n    };\r\n\r\n    return (\r\n        <div className=\"detailsNext4DaysData\">\r\n            <table>\r\n                <tbody>\r\n                    {\r\n                        dailyWeather.map((value, i) => {\r\n                            if (i > 4) {\r\n                                return null;\r\n                                // console.log(\"Next 4 days rendered\");\r\n                            }\r\n                            else if (i > 0) {\r\n                                return (\r\n                                    <tr key={i}>\r\n                                        <td style={{width: \"60px\", paddingRight: \"10px\"}}>{getDayOfWeek(value.dt)}</td>\r\n                                        <td style={{width: \"110px\", paddingRight: \"10px\"}}>{value.weather[0].description}</td>\r\n                                        <td style={{paddingRight: \"10px\"}}><img alt=\"icon\" src={`http://openweathermap.org/img/w/${value.weather[0].icon}.png`}></img></td>\r\n                                        <td style={{color: \"#0e59c9\", paddingRight: \"10px\"}}>{rainChance(value)}</td>\r\n                                        <td style={{paddingRight: \"10px\"}}>{finalTemp(value.temp.day)}&#176;</td>\r\n                                        <td style={{color: \"grey\"}}>{finalTemp(value.temp.night)}&#176;</td>\r\n                                    </tr>\r\n                                )\r\n                            }\r\n                        })\r\n                    }\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default NextFourDays;\r\n\r\n",{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"63","replacedBy":"67"},{"ruleId":"65","replacedBy":"68"},{"ruleId":"63","replacedBy":"69"},{"ruleId":"65","replacedBy":"70"},{"ruleId":"63","replacedBy":"71"},{"ruleId":"65","replacedBy":"72"},{"ruleId":"63","replacedBy":"73"},{"ruleId":"65","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":38,"column":53,"nodeType":"77","messageId":"78","endLine":38,"endColumn":55},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],["79"],["80"],["79"],["80"],["79"],["80"],["79"],["80"],"array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","no-global-assign","no-unsafe-negation"]